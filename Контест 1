#include<iostream>
#include<vector>
#include<cmath>

using std::cin;
using std::cout;
using std::endl;
using std::vector;


struct stack {
	int value;
	stack* next;
};

void add_top(stack** top, int value) {
						
	if (*top == NULL) {
		*top = (stack*)malloc(sizeof(stack));
		(*top)->value = value;
		(*top)->next = NULL;
	}
	else {
		stack* a;
		a = (stack*)malloc(sizeof(stack));
		a->value = value;
		a->next = *top;
		*top = a;
	
	}
}

void pop_top(stack** top) {
	stack* a = *top;
	*top = a->next;
	free(a);
	a->value = NULL;
}



void print(stack* top) {
	stack* a = top;
	
	while (a != NULL) {
		cout << a->value << " ";
		a = a->next;
		
	

	}
	
	

}



int main() {
	vector<int>v;
	int a;
	cin >> a;
	while (a != 0) {
		v.push_back(a);
		cin >> a;
	}
	
	for (int& i : v) {
		//cout << i << " ";
	}



	stack* top;
	top = NULL;

	for (int i = 0; i < v.size() ; ++i) {
		if (v[i] > 0) {
			add_top(&top, v[i]);
			//cout << (*top).value;
		}
		
		if (v[i] < 0 and abs(v[i]) < (*top).value) {
			(*top).value = (*top).value - abs(v[i]);
			//cout << (*top).value << " ";
		}

		if (v[i] < 0 and abs(v[i]) >= (*top).value) {

			pop_top(&top);

			//cout << (*top).value << " ";
		}
		
		
		
	}

	print(top);
	





	return 0;
}
